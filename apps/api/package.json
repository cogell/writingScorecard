{
  "name": "@fast/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "clean": "rm -rf dist .wrangler",
    "db:migrate:local": "wrangler d1 migrations apply fast-scorecards --local",
    "db:migrate:remote": "wrangler d1 migrations apply fast-scorecards --remote"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.6",
    "@fast/shared": "workspace:*",
    "ai": "^4.1.16",
    "nanoid": "^5.0.9",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "typescript": "^5.7.2",
    "wrangler": "^3.99.0"
  }
}
