{
  "name": "@fast/client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "mkdir -p dist && (wrangler dev --port 8787 & vite)",
    "dev:vite": "vite",
    "dev:worker": "mkdir -p dist && wrangler dev --port 8787",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "deploy": "wrangler deploy",
    "clean": "rm -rf dist .wrangler",
    "db:migrate:local": "wrangler d1 migrations apply writing-scorecard-auth --local",
    "db:migrate:remote": "wrangler d1 migrations apply writing-scorecard-auth --remote",
    "secrets:push": "grep -v '^#' .dev.vars | grep . | while IFS='=' read -r key value; do printf '%s' \"$value\" | wrangler secret put \"$key\"; done"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.6",
    "ai": "^4.1.16",
    "better-auth": "^1.4.18",
    "drizzle-orm": "^0.45.1",
    "nanoid": "^5.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.16",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.8",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "wrangler": "^4.62.0"
  }
}
